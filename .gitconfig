[user]
	name = Akito Tabira
	email = akito.tabira@gmail.com
[core]
	editor = vim
	autocrlf = input
[github]
	user = mythosil
[color]
	ui = auto
[alias]
	ci = commit
	co = checkout
	tree = log --graph --all --decorate
	init-shared = init --bare --shared
	treeline = log --all --graph --decorate --oneline
	br = branch
	conf = config
	st = status
	d = diff
	di = diff
	unstage = reset HEAD
	pom = push origin master
[log]
	decorate = short
[pull]
	rebase = true
[ghq]
  root = ~/git
